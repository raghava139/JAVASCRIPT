    <script>
    //setTimeout
    //setInterval
    //clearTimeout
    //clearInterval
        var mov;
        function mov(){
            console.log('This is time out');
        }

        let y=setTimeout(mov,5000,'hello');
        clearTimeout(y);
       
        let s=setInterval(mov,1000,'msg');
      
        clearInterval(s);


        
    </script>
